  <owl:Class rdf:about="#Threshold"/>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#ThresholdAction -->

  <owl:Class rdf:about="#ThresholdAction">
    <rdfs:subClassOf rdf:resource="#Threshold"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasThresholdValue"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasThresholdMetric"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;string"
        >Previously called &quot;Jmp&quot; (jump)
    </rdfs:comment>
  </owl:Class>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#ThresholdValueClass -->

  <owl:Class rdf:about="#ThresholdValueClass">
    <rdfs:subClassOf rdf:resource="#Threshold"/>
    <rdfs:comment rdf:datatype="&xsd;string"
