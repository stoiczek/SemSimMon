         xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
         xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
         xmlns:metric="http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#"
         xmlns:resource="http://www.icsr.agh.edu.pl/ontologies/monitoring/resource#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.icsr.agh.edu.pl/ontologies/monitoring/resource"/>
  </owl:Ontology>


  <!--
 ///////////////////////////////////////////////////////////////////////////////////////
 //
 // Object Properties
 //
 ///////////////////////////////////////////////////////////////////////////////////////
  -->


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasThresholdAction -->

  <owl:ObjectProperty rdf:about="#hasThresholdAction">
    <rdfs:domain rdf:resource="#AbstractMetric"/>
    <rdfs:range rdf:resource="#ThresholdAction"/>
    <rdfs:subPropertyOf rdf:resource="#thresholdProperty"/>
  </owl:ObjectProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasThresholdMetric -->

  <owl:ObjectProperty rdf:about="#hasThresholdMetric">
    <rdfs:comment rdf:datatype="&xsd;string"
        >DynamicMetric in range will be probably never used
    </rdfs:comment>
    <rdfs:domain rdf:resource="#ThresholdAction"/>
    <rdfs:subPropertyOf rdf:resource="#thresholdProperty"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#AbstractMetric"/>
          <rdf:Description rdf:about="#DynamicMetric"/>
          <rdf:Description rdf:about="&owl;Class"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasThresholdValue -->

  <owl:ObjectProperty rdf:about="#hasThresholdValue">
    <rdfs:domain rdf:resource="#ThresholdAction"/>
    <rdfs:range rdf:resource="#ThresholdValueClass"/>
    <rdfs:subPropertyOf rdf:resource="#thresholdProperty"/>
  </owl:ObjectProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#monitors -->

  <owl:ObjectProperty rdf:about="#monitors">
    <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    <rdfs:range rdf:resource="&owl;Class"/>
  </owl:ObjectProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#thresholdProperty -->

  <owl:ObjectProperty rdf:about="#thresholdProperty"/>


  <!--
 ///////////////////////////////////////////////////////////////////////////////////////
 //
 // Data properties
 //
 ///////////////////////////////////////////////////////////////////////////////////////
  -->


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasAutomaticRunsCount -->

  <owl:DatatypeProperty rdf:about="#hasAutomaticRunsCount">
    <rdfs:subPropertyOf rdf:resource="#hasRankingProperty"/>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasAverageUserRankValue -->

  <owl:DatatypeProperty rdf:about="#hasAverageUserRankValue">
    <rdfs:subPropertyOf rdf:resource="#hasRankingProperty"/>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasCustomImplementationClass -->

  <owl:DatatypeProperty rdf:about="#hasCustomImplementationClass">
    <rdfs:comment rdf:datatype="&xsd;string"
        >Contains fully qualified Java class name of custom metric implementation.
    </rdfs:comment>
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#AbstractMetric"/>
          <rdf:Description rdf:about="&owl;Thing"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasDatatypeThresholdValue -->

  <owl:DatatypeProperty rdf:about="#hasDatatypeThresholdValue">
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    <rdfs:domain rdf:resource="#ThresholdValueClass"/>
    <rdfs:range rdf:resource="&xsd;int"/>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasManualRunsCount -->

  <owl:DatatypeProperty rdf:about="#hasManualRunsCount">
    <rdfs:subPropertyOf rdf:resource="#hasRankingProperty"/>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasRankingProperty -->

  <owl:DatatypeProperty rdf:about="#hasRankingProperty">
    <rdfs:domain rdf:resource="#AbstractMetric"/>
    <rdfs:range rdf:resource="&xsd;string"/>
  </owl:DatatypeProperty>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#hasUserRanksCount -->

  <owl:DatatypeProperty rdf:about="#hasUserRanksCount">
    <rdfs:subPropertyOf rdf:resource="#hasRankingProperty"/>
  </owl:DatatypeProperty>


