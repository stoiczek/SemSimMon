  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#AbstractMetric -->

  <owl:Class rdf:about="#AbstractMetric">
    <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasThresholdAction"/>
        <owl:allValuesFrom rdf:resource="#ThresholdAction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#Axiom_1 -->

  <owl:Class rdf:about="#Axiom_1">
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#AbstractMetric"/>
          <rdf:Description rdf:about="&owl;Thing"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>


  <!-- http://www.icsr.agh.edu.pl/ontologies/monitoring/metric#DynamicMetric -->

  <owl:Class rdf:about="#DynamicMetric">
